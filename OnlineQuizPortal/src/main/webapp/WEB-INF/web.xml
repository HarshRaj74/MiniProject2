<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddQuestionServlet</servlet-name>
  	<display-name>AddQuestionServlet</display-name>
  	<description></description>
  	<servlet-class>com.quiz.controller.AddQuestionServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QuestionListServlet</servlet-name>
    <servlet-class>com.quiz.controller.QuestionListServlet</servlet-class>
</servlet>

  <servlet-mapping>
  	<servlet-name>AddQuestionServlet</servlet-name>
  	<url-pattern>/AddQuestionServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>QuestionListServlet</servlet-name>
  	<url-pattern>/QuestionListServlet</url-pattern>
  </servlet-mapping>
  

<servlet-mapping>
    <servlet-name>QuestionListServlet</servlet-name>
    <url-pattern>/QuestionList</url-pattern>
</servlet-mapping>
  
</web-app>
